<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/listItemfeedTitleTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="22dp"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="15dp"
        android:text="TextView"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:text="Feed Name" />

    <TextView
        android:id="@+id/listItemFeedUrlTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/listItemfeedTitleTextView"
        android:layout_alignParentTop="true"
        android:layout_marginTop="54dp"
        android:text="feed Url : http://www.feedme.no"
        android:layout_alignLeft="@+id/listItemfeedTitleTextView" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="82dp"
        android:layout_marginEnd="17dp"
        android:layout_marginRight="17dp"
        android:text="Tap to delete"
        android:textColor="@color/bluecolor" />
</RelativeLayout>
